    <Application x:Class="MiniTwitter.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    ShutdownMode="OnExplicitShutdown"
    Startup="App_Startup" Exit="App_Exit" SessionEnding="App_SessionEnding" DispatcherUnhandledException="App_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source=".\Resources\Common.xaml"/>
                <ResourceDictionary Source=".\Resources\Templates.xaml"/>
                <ResourceDictionary Source=".\Emoji\EmojiImages.xaml"/>
                <!--<ResourceDictionary Source=".\Resources\Translation\ja-JP.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
